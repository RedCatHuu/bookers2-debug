 <!--<div class="search_form">-->
  <%= form_with url: search_path, method: :get, local: true do |f|  %>
    <%= f.text_field :word, style: "width: 50%;"%>
    <%= f.select :model, [["User"], ["Book"]] %>
    <%= f.select :how, [["完全一致", 1],["前方一致", 2],["後方一致", 3], ["部分一致", 4]] %>
    <%= f.submit '検索'%>
  <% end %>
